package utils;

public class SqlUtils {
	private SqlUtils() {
		
	}
	
	public static String md5(String password) {
		
	}
	
	public static void close(AutoCloseable ...closeables) {
		for(AutoCloseable closeable: closeables) {
			if(closeable != null) {
				try {
					closeable.close();
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		}
	}
}
